{
  "name": "tabris-decorators",
  "version": "3.3.0",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eclipsesource/tabris-decorators.git"
  },
  "license": "BSD-3-Clause",
  "scripts": {
    "clean": "rm -rf ./dist/*",
    "build": "webpack --mode=production && npm run dts",
    "dts": "dts-generator --name tabris-decorators --project ./tsconfig-dts.json --out ./dist/index.d.ts --main tabris-decorators/index",
    "test": "npm run lint && ts-mocha -p ./tsconfig-test.json ./test/*.spec.ts ./test/*.spec.tsx",
    "test:file": "ts-mocha -p ./tsconfig-test.json",
    "lint": "tslint --project ./tsconfig-test.json src/*/*.ts test/**/*.ts test/**/*.tsx",
    "safePublish": "npm test && npm run build && npm publish",
    "start": "run-script-os",
    "start:nix": "./launch.sh",
    "start:windows": ".\\launch.cmd"
  },
  "peerDependencies": {
    "tabris": "^3.3.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.0",
    "@types/lodash": "^4.14.123",
    "@types/mocha": "^5.2.7",
    "@types/sinon": "^7.0.13",
    "@types/sinon-chai": "^3.2.3",
    "ajv-cli": "^2.1.0",
    "chai": "^3.5.0",
    "dts-generator": "^2.1.0",
    "install": "^0.12.2",
    "mocha": "^6.2.0",
    "npm": "^6.9.0",
    "run-script-os": "^1.0.7",
    "sinon": "^7.4.1",
    "sinon-chai": "^3.3.0",
    "tabris": "nightly",
    "ts-loader": "^6.0.4",
    "ts-mocha": "^6.0.0",
    "tslint": "^5.18.0",
    "typescript": "3.3.x",
    "webpack": "^4.0.0",
    "webpack-cli": "^3.2.3",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "reflect-metadata": "^0.1.13"
  }
}
